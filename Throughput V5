	<XMLAttributes><XMLAttribute name="RS_CreateExtendedDataItems" value="true" output="no"/><XMLAttribute name="RS_modelModificationTime" value="2015-01-29T21:59:28.244Z" output="no"/><XMLAttribute name="listSeparator" value="," output="no"/></XMLAttributes><reportName>Throughput</reportName><modelPath>/content/folder[@name='lululemon packages']/package[@name='WMOS']/model[@name='2015-01-29T21:59:28.146Z']</modelPath><queries><query name="UnitsPicked">
			<source>
				<sqlQuery name="SQL2" dataSource="WMOS">
					<sqlText>SELECT
          ptt.begin_date as begin_time

   , ptt.user_id
    , to_number(#prompt('pGoal')#)/round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
    - trunc(sysdate)  )*24,0) as Goal
 
   , o.tc_order_id as Order_ID,

wm.ptt.CREATE_DATE_TIME  + INTERVAL '3' HOUR AS "Process Date",
to_char(wm.ptt.CREATE_DATE_TIME + INTERVAL '3' HOUR, 'HH24') as "Hour",



CASE 
        WHEN wm.oli.invn_type = 'E' and wm.o.TOTAL_NBR_OF_UNITS = 1 THEN 'Ecom - Single'
        WHEN wm.oli.invn_type = 'E' and wm.o.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'Ecom - Multi'
        
        WHEN wm.oli.invn_type = 'R' and wm.o.TOTAL_NBR_OF_UNITS = 1 THEN 'One Inventory - Single'
        WHEN wm.oli.invn_type = 'R' and wm.o.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'One Inventory - Multi'
    ELSE NULL
END AS Order_Type,


    SUM(NVL(wm.ptt.nbr_units, 0)) as "Units Picked",
    wm.lh.area || wm.lh.zone as Location

	-- Added to filter Milt vs Miss
	,  CASE
    WHEN wm.lh.area || wm.lh.zone IN ('AS1','AS2','AS3','PA1','PA2','PA3') THEN 'Mississauga'
    ELSE 'Milton'
    END AS Facility,
    CASE 
    WHEN WM.ifmw.pick_locn_assign_type = 'P09' THEN 'Footwear'
    ELSE 'Non-Footwear'
END AS Footwear_Indicator


FROM
           wm.orders o
    LEFT JOIN wm.order_line_item oli ON wm.o.order_id = wm.oli.order_id
    LEFT JOIN wm.do_status ON wm.o.do_status = wm.do_status.order_status
    LEFT JOIN WM.item_cbo ic ON wm.oli.item_name = WM.ic.item_name
    LEFT JOIN WM.item_facility_mapping_wms ifmw ON wm.oli.item_id = WM.ifmw.item_id
 
   inner join wm.prod_trkg_tran ptt on wm.ptt.tc_order_id = wm.o.tc_order_id
    and wm.oli.line_item_id = wm.ptt.line_item_id
    AND  wm.ptt.tran_type = '500' and wm.ptt.tran_code = '010'
    INNER join wm.locn_hdr lh on wm.ptt.from_locn = wm.lh.locn_id


WHERE
        --o.order_type = ('ECOMM') and lh.area||lh.zone like ('MH1%') or lh.area||lh.zone like ('MH0%') and

   wm.o.order_type = 'ECOMM'
 -- AND wm.do_status.order_status = 130
 AND wm.oli.DO_DTL_STATUS NOT IN ('200')
 AND wm.lh.area || wm.lh.zone NOT IN ('PAP','HS1','ECM','HM1','AGI')
  AND wm.lh.area || wm.lh.zone IS NOT NULL

--and wm.prod_trkg_tran.CREATE_DATE_TIME &gt; to_date('09-10-2024 02:00:00','DD-MM-YYYY HH24:MI:SS') 
AND wm.ptt.CREATE_DATE_TIME &gt;= cast(to_date(substr(#Prompt('pStartDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') - INTERVAL '3' HOUR as timestamp)
AND wm.ptt.CREATE_DATE_TIME &lt;= cast(to_date(substr(#Prompt('pEndDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') - INTERVAL '3' HOUR as timestamp)



GROUP BY

       ic.item_bar_code,ifmw.PICK_LOCN_ASSIGN_TYPE,ic.product_class_id, lh.area||lh.zone,oli.invn_type,lh.work_grp, ptt.nbr_units ,wm.ptt.CREATE_DATE_TIME,

	ptt.begin_date,
	o.order_type,
	ptt.user_id,  o.tc_order_id ,
	lh.area||lh.zone
	, o.TOTAL_NBR_OF_UNITS
	, (CASE
    WHEN wm.lh.area || wm.lh.zone IN ('AS1','AS2','AS3','PA1','PA2','PA3') THEN 'Mississauga'
    ELSE 'Milton'
    END ),
    CASE 
    WHEN WM.ifmw.pick_locn_assign_type = 'P09' THEN 'Footwear'
    ELSE 'Non-Footwear'
END 
ORDER BY
    ptt.begin_date desc</sqlText>
					<mdProjectedItems><mdProjectedItem name="BEGIN_TIME"/><mdProjectedItem name="USER_ID"/><mdProjectedItem name="GOAL"/><mdProjectedItem name="ORDER_ID"/><mdProjectedItem name="Process Date"/><mdProjectedItem name="Hour"/><mdProjectedItem name="ORDER_TYPE"/><mdProjectedItem name="Units Picked"/><mdProjectedItem name="LOCATION"/><mdProjectedItem name="FACILITY"/><mdProjectedItem name="FOOTWEAR_INDICATOR"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="Process Date"><expression>[SQL2].[Process Date]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL2].[FACILITY]</expression></dataItem><dataItem name="Hour"><expression>[SQL2].[Hour]</expression></dataItem><dataItem name="ORDER_TYPE"><expression>[SQL2].[ORDER_TYPE]</expression></dataItem><dataItem name="Units Picked"><expression>[SQL2].[Units Picked]</expression></dataItem><dataItem name="Pickers"><expression>[SQL2].[USER_ID]</expression></dataItem><dataItem name="GOAL" aggregate="maximum"><expression>[SQL2].[GOAL]</expression></dataItem><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL2].[FOOTWEAR_INDICATOR]</expression></dataItem><dataItem name="ORDER_ID"><expression>[SQL2].[ORDER_ID]</expression></dataItem></selection>
			<queryHints><localCache value="false"/></queryHints><detailFilters><detailFilter use="optional" postAutoAggregation="false"><filterExpression>[SQL2].[FACILITY] in ?pFacility?</filterExpression></detailFilter><detailFilter><filterExpression>[SQL2].[FOOTWEAR_INDICATOR] in ?pFootwearIndicator?</filterExpression></detailFilter></detailFilters></query><query name="UnitsPacked">
			<source>
				<sqlQuery name="SQL1" dataSource="WMOS">
					<sqlText>WITH RankedData AS (
SELECT
   ptt.begin_date+wh.TIME_ZONE as begin_time
   , to_number(#prompt('pGoal')#)/round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
	- trunc(sysdate)  )*24,0) as Goal,
        user_id,
        ptt.cntr_nbr,
        TO_CHAR(sysdate + wh.UTC_ADJ, 'MM/DD HH24:MI:SS') AS sys_time,
        TO_CHAR(ptt.begin_date + wh.TIME_ZONE, 'MM/DD/YYYY') AS "Process Date",
        TO_CHAR(ptt.begin_date + wh.TIME_ZONE, 'HH24') AS "Hour",
        CASE 
            WHEN o.TOTAL_NBR_OF_UNITS = 1 THEN 'Ecomm - Single' 
            ELSE 'Ecomm - Multi' 
        END AS "Order Type",
        ptt.nbr_units AS "Units Packed",
        CASE 
            WHEN s.SHIFT_CODE IN ('1M', '2M', '3M') THEN 'Milton'
            ELSE 'Mississauga'
        END AS FACILITY,
        CASE 
            WHEN pc.product_class IN ('118','441','442','820','821','836','834','835','837') THEN 'Footwear'
            ELSE 'Non-Footwear'
        END AS footwear_indicator,
        ROW_NUMBER() OVER (PARTITION BY ptt.cntr_nbr ORDER BY ptt.begin_date DESC) AS row_num
    FROM
        wm.prod_trkg_tran ptt
        INNER JOIN wm.orders o ON ptt.tc_order_id = o.tc_order_id
        LEFT OUTER JOIN (
            SELECT DISTINCT order_id, invn_type, line_item_id 
            FROM order_line_item
        ) oli ON o.order_id = oli.order_id
        LEFT OUTER JOIN item_cbo ic ON ic.item_id = ptt.item_id
        LEFT OUTER JOIN (
            SELECT DISTINCT locn_id, area, zone 
            FROM wm.locn_hdr
        ) lh ON ptt.from_locn = lh.locn_id
        LEFT OUTER JOIN product_class pc ON ic.product_class_id = pc.product_class_id
        LEFT OUTER JOIN wm.lpn l ON l.tc_order_id = ptt.cntr_nbr
        LEFT OUTER JOIN wm.lpn_detail ld ON ld.lpn_id = l.lpn_id AND oli.line_item_id = ld.DISTRIBUTION_ORDER_DTL_ID
        LEFT OUTER JOIN (
            SELECT 
                uu.user_name, 
                ed.emp_id, 
                s.shift_code, 
                ed.shift_id, 
                SUBSTR(s.shift_code, 0, 1) AS Shift 
            FROM 
                (
                    SELECT 
                        emp_id, 
                        shift_id, 
                        RANK() OVER (PARTITION BY emp_id ORDER BY MAX(eff_date_time) DESC) AS rnk 
                    FROM wm.e_emp_dtl 
                    WHERE eff_date_time &lt;= TRUNC(SYSDATE) 
                    GROUP BY emp_id, shift_id
                ) ed
            INNER JOIN wm.ucl_user uu ON ed.emp_id = uu.ucl_user_id
            INNER JOIN wm.e_shift s ON ed.shift_id = s.shift_id
            WHERE rnk = 1
        ) s ON ptt.user_id = s.user_name
        LEFT OUTER JOIN (
            SELECT DISTINCT tc_order_id, invn_need_type, line_item_id, carton_nbr, task_type, task_id, invn_type
            FROM wm.task_dtl
        ) td ON (
            (td.tc_order_id = o.tc_order_id AND ld.DISTRIBUTION_ORDER_DTL_ID = td.line_item_id AND td.invn_need_type = '55')
            OR (td.carton_nbr = ptt.cntr_nbr AND ld.DISTRIBUTION_ORDER_DTL_ID = td.line_item_id AND td.invn_need_type = '50')
        )
        CROSS JOIN (
            SELECT 
                SUM(CASE WHEN TRIM(pay_scale) = 'TIME_ZONE' THEN TO_NUMBER(pay_scale_desc)/24 END) AS TIME_ZONE,
                SUM(CASE WHEN TRIM(pay_scale) = 'UTC_ADJ' THEN TO_NUMBER(pay_scale_desc)/24 END) AS UTC_ADJ,
                SUM(CASE WHEN TRIM(pay_scale) = 'SHIFT_ADJ' THEN TO_NUMBER(pay_scale_desc)/24 END) AS SHIFT_ADJ 
            FROM wm.e_pay_scale 
            WHERE pay_scale IN ('TIME_ZONE', 'SHIFT_ADJ', 'UTC_ADJ')
        ) wh
    WHERE
        ptt.nbr_units &gt; 0
        AND ptt.menu_optn_name IN (
            'Weigh and Manifest oLPN',
            'Pck From Tote',
            'Pck From Tote AB',
            'Pck From Tote SA',
            'Pck From Tote PUTWALL'
        )
        AND o.order_type = 'ECOMM'
        AND ptt.user_id not IN ('WMADMIN', 'MHEADMIN')
        and cast(ptt.begin_date+wh.TIME_ZONE as timestamp) &gt;= cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)
	and cast(ptt.begin_date+wh.TIME_ZONE as timestamp) &lt;= cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)
        AND ptt.CREATE_DATE_TIME &gt;= SYSDATE - (49 / 24)
        AND ptt.CREATE_DATE_TIME &lt;= SYSDATE
)
SELECT *
FROM RankedData
WHERE row_num = 1
ORDER BY begin_time DESC</sqlText>
					<mdProjectedItems><mdProjectedItem name="BEGIN_TIME"/><mdProjectedItem name="GOAL"/><mdProjectedItem name="USER_ID"/><mdProjectedItem name="CNTR_NBR"/><mdProjectedItem name="SYS_TIME"/><mdProjectedItem name="Process Date"/><mdProjectedItem name="Hour"/><mdProjectedItem name="Order Type"/><mdProjectedItem name="Units Packed"/><mdProjectedItem name="FACILITY"/><mdProjectedItem name="FOOTWEAR_INDICATOR"/><mdProjectedItem name="ROW_NUM"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="Process Date"><expression>[SQL1].[Process Date]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL1].[FACILITY]</expression></dataItem><dataItem name="Hour"><expression>[SQL1].[Hour]</expression></dataItem><dataItem name="Order Type"><expression>[SQL1].[Order Type]</expression></dataItem><dataItem name="Units Packed" aggregate="total"><expression>[SQL1].[Units Packed]</expression></dataItem><dataItem name="Packers"><expression>[SQL1].[USER_ID]</expression></dataItem><dataItem name="GOAL" aggregate="maximum"><expression>[SQL1].[GOAL]</expression></dataItem><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL1].[FOOTWEAR_INDICATOR]</expression></dataItem></selection>
			<queryHints><localCache value="false"/></queryHints><detailFilters><detailFilter use="optional" postAutoAggregation="false"><filterExpression>[SQL1].[FACILITY] in ?pFacility?</filterExpression></detailFilter><detailFilter><filterExpression>[SQL1].[FOOTWEAR_INDICATOR] in ?pFootwearIndicator?</filterExpression></detailFilter></detailFilters></query><query name="UnitsPut">
			<source>
				<sqlQuery name="SQL3" dataSource="WMOS">
					<sqlText>select
   lm.sched_start_time+wh.TIME_ZONE  as begin_time
   , lm.login_user_id
   , to_char(sysdate+wh.UTC_ADJ  , 'MM/DD HH24:MI:SS') as sys_time,
   to_char(lm.sched_start_time+wh.TIME_ZONE , 'MM/DD/YYYY') as "Process Date",
   to_char(lm.sched_start_time+wh.TIME_ZONE  , 'HH24') as "Hour",
   sum(lmd.qty) as "Units Put"
   , case when usr.shift_code like '%M' then 'Milton' else 'Mississauga' end as FACILITY
   , case when pc.product_class in ('118','441','442','820','821','836','834','835','837') then 'Footwear' else 'Non-Footwear' end as footwear_indicator

from   
    wm.labor_msg lm 
    inner join wm.labor_msg_dtl lmd on lm.labor_msg_id = lmd.labor_msg_id
    left outer join wm.item_cbo ic on ic.item_name = lmd.item_name
    left outer join wm.product_class pc on ic.product_class_id = pc.product_class_id
    

left outer join (
    select uu.user_name, ed.emp_id
        , max(d.shift_code) shift_code
        --, ed.shift_id, substr(s.shift_code,0,1) as Shift 
        --, uu.is_active
        , max(ed.eff_date_time) eff_date_time
    from(
        select emp_id, shift_id, rank() over (partition by EMP_ID order by MAX(EFF_DATE_TIME) desc) as rnk 		
            , eff_date_time
        from wm.e_emp_dtl 
        where eff_date_time &lt;= trunc(sysdate) 
            -- TESTING 
            --and emp_id = '15677'
        group by emp_id, shift_id
            , eff_date_time
        ) ed 
    inner join wm.ucl_user uu on ed.emp_id = uu.ucl_user_id 
    inner join wm.e_shift d on ed.shift_id = d.shift_id 
    where --rnk =1
        -- TESTING
        d.shift_code like '%M'
    group by uu.user_name, ed.emp_id
    ) usr on lm.login_user_id = usr.user_name

, (  SELECT sum(case when trim(pay_scale) = 'TIME_ZONE' then to_number(pay_scale_desc)/24 end) as TIME_ZONE, sum(case when trim(pay_scale) = 'UTC_ADJ' then to_number(pay_scale_desc)/24 end) as UTC_ADJ, sum(case when trim(pay_scale) = 'SHIFT_ADJ' then to_number(pay_scale_desc)/24 end) as SHIFT_ADJ FROM wm.e_pay_scale WHERE pay_scale in ('TIME_ZONE','SHIFT_ADJ','UTC_ADJ')  )  wh
where
   lmd.qty &gt;0
   and lm.act_name in ('02-0422','02-0322','02-0320')
   and cast(lm.sched_start_time+wh.TIME_ZONE as timestamp)  &gt;= cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)
   and cast(lm.sched_start_time+wh.TIME_ZONE as timestamp)  &lt;= cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)

group by
   	lm.sched_start_time,
   	lm.login_user_id,
	wh.TIME_ZONE,
	wh.UTC_ADJ
    , case when usr.shift_code like '%M' then 'Milton' else 'Mississauga' end
    , case when pc.product_class in ('118','441','442','820','821','836','834','835','837') then 'Footwear' else 'Non-Footwear' end

order by
  lm.SCHED_START_TIME desc</sqlText>
					<mdProjectedItems><mdProjectedItem name="BEGIN_TIME"/><mdProjectedItem name="LOGIN_USER_ID"/><mdProjectedItem name="SYS_TIME"/><mdProjectedItem name="Process Date"/><mdProjectedItem name="Hour"/><mdProjectedItem name="Units Put"/><mdProjectedItem name="FACILITY"/><mdProjectedItem name="FOOTWEAR_INDICATOR"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="Process Date" rollupAggregate="none"><expression>[SQL3].[Process Date]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL3].[FACILITY]</expression></dataItem><dataItem name="Hour" rollupAggregate="none"><expression>[SQL3].[Hour]</expression></dataItem><dataItem name="Units Put" aggregate="total"><expression>[SQL3].[Units Put]</expression></dataItem><dataItem name="Put-ers" aggregate="countDistinct"><expression>[SQL3].[LOGIN_USER_ID]</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="Process Date" aggregateMethod="total" name="Total(Process Date)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="FACILITY" aggregateMethod="total" name="Total(FACILITY)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="Hour" aggregateMethod="total" name="Total(Hour)" label="Total"/><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL3].[FOOTWEAR_INDICATOR]</expression></dataItem></selection>
			<detailFilters><detailFilter use="optional" postAutoAggregation="false"><filterExpression>[SQL3].[FACILITY] in ?pFacility?</filterExpression></detailFilter><detailFilter><filterExpression>[SQL3].[FOOTWEAR_INDICATOR] in ?pFootwearIndicator?</filterExpression></detailFilter></detailFilters></query><query name="units_WIP">
			<source>
				
			<sqlQuery name="SQL4" dataSource="WMOS">
			<sqlText>SELECT
   Order_type
   , do_Status as Order_Status
   , pick_status as Order_Status_Dtl
   , count(distinct order_id) as orders
   , sum(picked_qty)-sum(packed_qty) as units_picked
   , sum(packed_qty) as units_packed
   , sum(order_qty) as units_ordered
	, FACILITY
    , footwear_indicator
FROM
            (
            SELECT
            CASE WHEN TOTAL_NBR_OF_UNITS &gt; 1 THEN 'Multi' ELSE 'Single' END as Order_type
            , order_date
            , DC
            , order_id
            , DO_Status
            , max(order_qty) as order_qty
            , sum(lpn_qty) as packed_qty
            , max(qty_pulld) as picked_qty
            , CASE WHEN DO_Status = '130 - DC Allocated'
                THEN ( CASE  WHEN max(curr_locn) = 'ECM-00-00-PRE' THEN '(4) in VAS'
                              WHEN max(curr_locn) = 'ECM-00-00-TAG' THEN '(5) VAS Complete'
                              WHEN max(qty_pulld) = max(order_qty) THEN '(3) Pick Complete'
                              WHEN min(PT_Status) in ('05','5') THEN '(0) Pending FA'
                              WHEN min(PT_Status) = '10' THEN '(1) Ready to Carton Start'
                              WHEN min(PT_Status) in ('20','50') THEN '(2) Carton Started'
                              WHEN min(PT_Status) = '00' THEN '(0)'
                              --WHEN max(qty_pulld) &gt; sum(lpn_qty) THEN ' | (1) Pick Started'                                               
                           ELSE null
                           END)
            WHEN DO_Status = '160 - Weighed' 
                THEN (CASE WHEN DC in ('TOR','DEL')  THEN '(0) Waiting to be loaded on trailer' ELSE  '(0) Waiting on Ship Confirm' END )                       
            WHEN DO_Status = '180 - Loaded' THEN '(0) Waiting on shipment close'
            ELSE null END as Pick_Status
	, FACILITY
    , footwear_indicator

            FROM
                        (
                        SELECT
                           substr(o.o_facility_alias_id,0,3) as DC
                           , to_char(trunc(o.created_dttm+wh.UTC_ADJ),'YYYY/MM/DD') as order_date                        
                           , o.tc_order_id as order_id
                           , o.do_status||' - '||do.description as DO_Status
                           , oli.order_qty
                           , l.tc_lpn_id as LPN
                           , l.lpn_facility_status||' - '||lfs.description as LPN_Status
                           , ld.size_value as lpn_qty
                           , td.qty_pulld
                           , o.TOTAL_NBR_OF_UNITS
                           , nvl(l.misc_instr_code_3,'00') as PT_Status
                           , lh.dsp_locn as LPN_Locn
                           , dt.tote
                           , dt.prev_locn
                           , dt.curr_locn
		-- Added to filter Milt vs Miss
		, case when substr(o.o_facility_alias_id,1,3) = 'TOR' then
		(case when oli.invn_type = 'E' then 'Mississauga' when oli.invn_type = 'R' then 'Milton' end)
		else substr(o.o_facility_alias_id,1,3) end as FACILITY
        , case when pc.product_class in ('118','441','442','820','821','836','834','835','837') then 'Footwear' else 'Non-Footwear' end as footwear_indicator
                                
                        FROM
                        wm.orders o
                        inner join (select order_id, sum(order_qty) as order_qty, invn_type
		from wm.order_line_item 
		group by order_id
			, invn_type
		) oli on o.order_id = oli.order_id
                        inner join wm.order_line_item oli2 on oli2.order_id = o.order_id
                        inner join wm.item_cbo ic on ic.item_id = oli2.item_id
                        left outer join wm.product_class pc on ic.product_class_id = pc.product_class_id
                        left outer join wm.lpn l on o.tc_order_id = l.tc_order_id and l.lpn_facility_status &lt; '99'
                        left outer join wm.locn_hdr lh on l.curr_sub_locn_id = lh.locn_id
                        left outer join (select lpn_id, sum(size_value) as size_value from wm.lpn_detail group by lpn_id) ld on l.lpn_id = ld.lpn_id
                        left outer join (select tc_order_id, sum(qty_pulld) as qty_pulld from wm.task_dtl where stat_code &lt;= '90' group by tc_order_id) td on o.tc_order_id = td.tc_order_id
                        left outer join ( 
		select
                                               distinct td.tc_order_id
                                               , t.tc_lpn_id as tote
                                                , lp.dsp_locn as prev_locn
                                                , lc.dsp_locn as curr_locn
		from wm.lpn t
		inner join wm.task_dtl td on t.tc_lpn_id = td.tote_nbr
		left outer join wm.locn_hdr lp on t.prev_sub_locn_id = lp.locn_id
		left outer join wm.locn_hdr lc on t.curr_sub_locn_id = lc.locn_id
		where
                                                lp.dsp_locn in ('ECM-00-00-TAG','ECM-00-00-PRE')
                                                or lc.dsp_locn in ('ECM-00-00-TAG','ECM-00-00-PRE')
                                                ) dt on o.tc_order_id = dt.tc_order_id
                        inner join wm.do_status do on o.do_status = do.order_status
                        left outer join wm.lpn_facility_status lfs on l.lpn_facility_status = lfs.lpn_facility_status and lfs.inbound_outbound_indicator = 'O'
                        , ( SELECT sum(case when trim(pay_scale) = 'TIME_ZONE' then to_number(pay_scale_desc)/24 end) as TIME_ZONE, sum(case when trim(pay_scale) = 'UTC_ADJ' then to_number(pay_scale_desc)/24 end) as UTC_ADJ, sum(case when trim(pay_scale) = 'SHIFT_ADJ' then to_number(pay_scale_desc)/24 end) as SHIFT_ADJ FROM wm.e_pay_scale WHERE pay_scale in ('TIME_ZONE','SHIFT_ADJ','UTC_ADJ')  ) wh
                        WHERE
                        o.do_status &lt; '190'
                        and o.order_type = 'ECOMM'
                        )
            GROUP BY
            TOTAL_NBR_OF_UNITS
            , order_date
            , DC
            , order_id
            , DO_Status
	, FACILITY
    , footwear_indicator
            )
GROUP BY
Order_type
, do_Status
, pick_status
	, FACILITY
    , footwear_indicator

ORDER BY
do_Status asc
, pick_status asc</sqlText>
