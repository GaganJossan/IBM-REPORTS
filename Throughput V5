			<selection><dataItem name="ORDER_TYPE"><expression>[SQL4].[ORDER_TYPE]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL4].[FACILITY]</expression></dataItem><dataItem name="ORDER_STATUS"><expression>[SQL4].[ORDER_STATUS]</expression></dataItem><dataItem name="ORDER_STATUS_DTL"><expression>[SQL4].[ORDER_STATUS_DTL]</expression></dataItem><dataItem name="UNITS_PICKED"><expression>[SQL4].[UNITS_PICKED]</expression></dataItem><dataItem name="UNITS_PACKED"><expression>[SQL4].[UNITS_PACKED]</expression></dataItem><dataItem name="UNITS_ORDERED" aggregate="total"><expression>[SQL4].[UNITS_ORDERED]</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="ORDER_TYPE" aggregateMethod="total" name="Total(ORDER_TYPE)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="ORDER_STATUS" aggregateMethod="total" name="Total(ORDER_STATUS)" label="Total"/><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL4].[FOOTWEAR_INDICATOR]</expression></dataItem></selection>
		<detailFilters><detailFilter><filterExpression>[SQL4].[FOOTWEAR_INDICATOR] in ?pFootwearIndicator?</filterExpression></detailFilter></detailFilters></query><query name="thruput_smry">
			<source>
				
			<sqlQuery name="SQL6" dataSource="WMOS">
			<sqlText>select
   to_number(#prompt('pGoal')#) as goal
   , round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
	- trunc(sysdate)  )*24,0) as hrs
   , CASE WHEN cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) &lt; cast(sysdate+wh.UTC_ADJ  as timestamp) 
	THEN round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
	- trunc(sysdate)  )*24,0) ELSE round((  trunc(sysdate) + (   cast(sysdate+wh.UTC_ADJ  as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
	- trunc(sysdate)  )*24,0)  END as Completed_Hrs
   , to_number(#prompt('pGoal')#) / round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
	- trunc(sysdate)  )*24,0) as HRLY_GOAL
from dual
, (  SELECT sum(case when trim(pay_scale) = 'TIME_ZONE' then to_number(pay_scale_desc)/24 end) as TIME_ZONE
   , sum(case when trim(pay_scale) = 'UTC_ADJ' then to_number(pay_scale_desc)/24 end) as UTC_ADJ
   , sum(case when trim(pay_scale) = 'SHIFT_ADJ' then to_number(pay_scale_desc)/24 end) as SHIFT_ADJ 
FROM wm.e_pay_scale 
WHERE pay_scale in ('TIME_ZONE','SHIFT_ADJ','UTC_ADJ')   ) wh</sqlText>
		<mdProjectedItems><mdProjectedItem name="GOAL"/><mdProjectedItem name="HRS"/><mdProjectedItem name="COMPLETED_HRS"/><mdProjectedItem name="HRLY_GOAL"/></mdProjectedItems></sqlQuery></source>
			<selection autoSummary="false"><dataItem name="GOAL"><expression>[SQL6].[GOAL]</expression></dataItem><dataItem name="HRLY_GOAL"><expression>[SQL6].[HRLY_GOAL]</expression></dataItem><dataItem name="COMPLETED_HRS"><expression>[SQL6].[COMPLETED_HRS]</expression></dataItem><dataItem name="HRS"><expression>[SQL6].[HRS]</expression></dataItem><dataItem name="PERC_TO_GOAL"><expression>[PACKED] / [GOAL]</expression></dataItem><dataItem name="UNITS_REMAINING"><expression>CASE WHEN [GOAL] - [PACKED] &lt; 0 THEN 0 ELSE [GOAL] - [PACKED]  END</expression></dataItem><dataItem name="PACKED"><expression> [GET_PACKED].[PACKED]</expression></dataItem><dataItem name="PACKED_ACTUAL_UPH"><expression>[PACKED] / [COMPLETED_HRS]</expression></dataItem><dataItem name="PACKED_PROJECTED_TOTAL"><expression>[PACKED] + ([PACKED_ACTUAL_UPH] * ([HRS] - [COMPLETED_HRS]) )</expression></dataItem><dataItem name="PACKED_ON_TRACK"><expression>CASE WHEN [HRLY_GOAL] &gt; [PACKED_ACTUAL_UPH] THEN 'Not on track' ELSE 'On track' END</expression></dataItem><dataItem name="PICKED"><expression>[GET_PICKED].[PICKED]</expression></dataItem><dataItem name="PICKED_ACTUAL_UPH"><expression>[PICKED] / [COMPLETED_HRS]</expression></dataItem><dataItem name="PICKED_PROJECTED_TOTAL"><expression>[PICKED] + ([PICKED_ACTUAL_UPH] * ([HRS] - [COMPLETED_HRS]) )</expression></dataItem><dataItem name="PICKED_ON_TRACK"><expression>CASE WHEN [HRLY_GOAL] &gt; [PICKED_ACTUAL_UPH] THEN 'Not on track' ELSE 'On track' END</expression></dataItem><dataItem name="PICKED_TO_PACKED_RATIO"><expression>[PICKED_ACTUAL_UPH] / [PACKED_ACTUAL_UPH]</expression></dataItem></selection>
		<queryHints><localCache value="false"/><crossProductAllowed value="allow"/></queryHints></query><query name="Query1">
			<source>
				
			<queryRef refQuery="UnitsPacked"/></source>
			<selection><dataItem name="Process Date" rollupAggregate="none"><expression>[UnitsPacked].[Process Date]</expression></dataItem><dataItem name="FACILITY"><expression>[UnitsPacked].[FACILITY]</expression></dataItem><dataItem name="Hour" rollupAggregate="none" sort="ascending"><expression>[UnitsPacked].[Hour]</expression></dataItem><dataItem name="Order Type" rollupAggregate="none"><expression>[UnitsPacked].[Order Type]</expression></dataItem><dataItem name="GOAL" aggregate="maximum"><expression>[UnitsPacked].[GOAL]</expression></dataItem><dataItem name="Units Packed" aggregate="total"><expression>[UnitsPacked].[Units Packed]</expression></dataItem><dataItem name="Packers" aggregate="countDistinct"><expression>[UnitsPacked].[Packers]</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="Order Type" aggregateMethod="total" name="Total(Order Type)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="FACILITY" aggregateMethod="total" name="Total(FACILITY)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="Process Date" aggregateMethod="total" name="Total(Process Date)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="Hour" aggregateMethod="total" name="Total(Hour)" label="Total"/><dataItem name="FOOTWEAR_INDICATOR"><expression>[UnitsPacked].[FOOTWEAR_INDICATOR]</expression></dataItem></selection>
		<queryHints><localCache value="false"/></queryHints></query><query name="Query2">
			<source>
				
			<queryRef refQuery="UnitsPicked"/></source>
			<selection><dataItem name="FACILITY"><expression>[UnitsPicked].[FACILITY]</expression></dataItem><dataItem name="Process Date" rollupAggregate="none"><expression>[UnitsPicked].[Process Date]</expression></dataItem><dataItem name="Hour" rollupAggregate="none" sort="ascending"><expression>[UnitsPicked].[Hour]</expression></dataItem><dataItem name="ORDER_TYPE"><expression>[UnitsPicked].[ORDER_TYPE]</expression></dataItem><dataItem name="Units Picked" aggregate="total"><expression>[UnitsPicked].[Units Picked]</expression></dataItem><dataItem name="Pickers" aggregate="countDistinct"><expression>[UnitsPicked].[Pickers]</expression></dataItem><dataItem name="GOAL" aggregate="maximum"><expression>[UnitsPicked].[GOAL]</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="FACILITY" aggregateMethod="total" name="Total(FACILITY)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="Process Date" aggregateMethod="total" name="Total(Process Date)" label="Total"/><dataItemEdgeSummary solveOrder="1" refDataItem="Hour" aggregateMethod="total" name="Total(Hour)" label="Total"/></selection>
		<queryHints><localCache value="false"/></queryHints></query><query name="GET_PICKED">
			<source>
				<model/>
			</source>
			<selection><dataItem name="PICKED" aggregate="maximum"><expression>total ( [UnitsPicked].[Units Picked] for report)</expression></dataItem></selection>
		<queryHints><localCache value="false"/></queryHints></query><query name="GET_PACKED">
			<source>
				<model/>
			</source>
			<selection><dataItem name="PACKED" aggregate="maximum"><expression>total ( [UnitsPacked].[Units Packed] for report)</expression></dataItem></selection>
		<queryHints><localCache value="false"/></queryHints></query><query name="FacilityChoose">
			<source>
				
			<sqlQuery name="SQL1" dataSource="WMOS">
			<sqlText>select case when rownum = 1 then 'Milton'
	when rownum = 2 then 'Mississauga'
	end FACILITY
from dual connect by rownum &lt;= 2</sqlText>
		<mdProjectedItems><mdProjectedItem name="FACILITY"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="FACILITY"><expression>[SQL1].[FACILITY]</expression></dataItem></selection>
		<queryHints><localCache value="false"/></queryHints></query><query name="FootwearSelect">
			<source>
				
			<sqlQuery name="SQL5" dataSource="WMOS">
			<sqlText>select case when rownum = 1 then 'Footwear'
	when rownum = 2 then 'Non-Footwear'
	end FOOTWEAR_INDICATOR
from dual connect by rownum &lt;= 2</sqlText>
		<mdProjectedItems><mdProjectedItem name="FOOTWEAR_INDICATOR"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL5].[FOOTWEAR_INDICATOR]</expression></dataItem></selection>
		</query><query name="PICK">
			<source>
				
			<sqlQuery name="SQL7" dataSource="WMOS">
			<sqlText>Select 
to_char(wm.prod_trkg_tran.CREATE_DATE_TIME  + INTERVAL '3' HOUR,   'MM/DD/YYYY')  AS Process_Date,
to_char(wm.prod_trkg_tran.CREATE_DATE_TIME + INTERVAL '3' HOUR, 'HH24') as Hour,

prod_trkg_tran.user_id
    , to_number(#prompt('pGoal')#)/round((  trunc(sysdate) 
	+ (   cast(to_date(substr(#Prompt('pEndDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp) 
	- cast(to_date(substr(#Prompt('pStartDateTime')#,0,19),'YYYY-MM-DD"T"HH24:mi:ss') as timestamp)    )  
    - trunc(sysdate)  )*24,0) as Goal,

--wm.orders.tc_order_id,
   -- wm.order_line_item.tc_order_line_id as DO_Line_Number,
CASE 
        WHEN wm.order_line_item.invn_type = 'E' and wm.orders.TOTAL_NBR_OF_UNITS = 1 THEN 'Ecom - Single'
        WHEN wm.order_line_item.invn_type = 'E' and wm.orders.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'Ecom - Multi'
        WHEN wm.order_line_item.invn_type = 'R' and wm.orders.TOTAL_NBR_OF_UNITS = 1 THEN 'One Inventory - Single'
        WHEN wm.order_line_item.invn_type = 'R' and wm.orders.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'One Inventory - Multi'
    ELSE NULL
END AS Order_Type,
    wm.order_line_item.invn_type as Order_Line_Inv_Type,
       -- wm.order_line_item.item_name,
    SUM(
        CASE
            WHEN WM.item_facility_mapping_wms.pick_locn_assign_type = 'P09' AND wm.prod_trkg_tran.tran_code IN ('006', '007') THEN NVL(wm.prod_trkg_tran.nbr_units, 0)
            WHEN WM.item_facility_mapping_wms.pick_locn_assign_type != 'P09' AND wm.prod_trkg_tran.tran_code = '010' THEN NVL(wm.prod_trkg_tran.nbr_units, 0)
            ELSE 0
        END
    ) AS Picked_QTY,
    wm.locn_hdr.area || wm.locn_hdr.zone as Location,
    
    CASE
    WHEN wm.locn_hdr.area || wm.locn_hdr.zone IN ('AS1','AS2','AS3','PA1','PA2','PA3') THEN 'Mississauga'
    ELSE 'Milton'
    END AS Facility,
    CASE 
    WHEN WM.item_facility_mapping_wms.pick_locn_assign_type = 'P09' THEN 'Footwear'
    ELSE 'Non-Footwear'
END AS Footwear_Indicator
FROM
    wm.orders
    LEFT JOIN wm.order_line_item ON wm.orders.order_id = wm.order_line_item.order_id
    LEFT JOIN wm.do_status ON wm.orders.do_status = wm.do_status.order_status
    LEFT JOIN WM.item_cbo ON wm.order_line_item.item_name = WM.item_cbo.item_name
    LEFT JOIN WM.item_facility_mapping_wms ON wm.order_line_item.item_id = WM.item_facility_mapping_wms.item_id
 
   inner join wm.prod_trkg_tran on wm.prod_trkg_tran.tc_order_id = wm.orders.tc_order_id
    and wm.order_line_item.line_item_id = wm.prod_trkg_tran.line_item_id
    AND  wm.prod_trkg_tran.tran_type = '500' and wm.prod_trkg_tran.tran_code IN ('010','006','007')
    INNER join wm.locn_hdr on wm.prod_trkg_tran.from_locn = wm.locn_hdr.locn_id

WHERE
   wm.orders.order_type = 'ECOMM'
 AND wm.order_line_item.DO_DTL_STATUS NOT IN ('200')
 AND wm.locn_hdr.area || wm.locn_hdr.zone NOT IN ('PAP','HS1','ECM','HM1','AGI')

AND wm.prod_trkg_tran.CREATE_DATE_TIME &gt;= cast(to_date(substr(#Prompt('pStartDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') - INTERVAL '3' HOUR as timestamp)
AND wm.prod_trkg_tran.CREATE_DATE_TIME &lt;= cast(to_date(substr(#Prompt('pEndDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') - INTERVAL '3' HOUR as timestamp)

GROUP BY
wm.prod_trkg_tran.CREATE_DATE_TIME  + INTERVAL '3' HOUR ,
to_char(wm.prod_trkg_tran.CREATE_DATE_TIME + INTERVAL '3' HOUR, 'HH24'), 
prod_trkg_tran.user_id,
--wm.orders.tc_order_id,
   -- wm.order_line_item.tc_order_line_id as DO_Line_Number,
CASE 
        WHEN wm.order_line_item.invn_type = 'E' and wm.orders.TOTAL_NBR_OF_UNITS = 1 THEN 'Ecom - Single'
        WHEN wm.order_line_item.invn_type = 'E' and wm.orders.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'Ecom - Multi'
        WHEN wm.order_line_item.invn_type = 'R' and wm.orders.TOTAL_NBR_OF_UNITS = 1 THEN 'One Inventory - Single'
        WHEN wm.order_line_item.invn_type = 'R' and wm.orders.TOTAL_NBR_OF_UNITS &gt; 1 THEN 'One Inventory - Multi'
    ELSE NULL
END,
    wm.order_line_item.invn_type,
     --   wm.order_line_item.item_name,
    wm.locn_hdr.area || wm.locn_hdr.zone,
    
    CASE
    WHEN wm.locn_hdr.area || wm.locn_hdr.zone IN ('AS1','AS2','AS3','PA1','PA2','PA3') THEN 'Mississauga'
    ELSE 'Milton'
    END,
    CASE
    WHEN WM.item_facility_mapping_wms.pick_locn_assign_type = 'P09' THEN 'Footwear'
    ELSE 'Non-Footwear'
END
Order By process_date</sqlText>
		<mdProjectedItems><mdProjectedItem name="PROCESS_DATE"/><mdProjectedItem name="HOUR"/><mdProjectedItem name="USER_ID"/><mdProjectedItem name="GOAL"/><mdProjectedItem name="ORDER_TYPE"/><mdProjectedItem name="ORDER_LINE_INV_TYPE"/><mdProjectedItem name="PICKED_QTY"/><mdProjectedItem name="LOCATION"/><mdProjectedItem name="FACILITY"/><mdProjectedItem name="FOOTWEAR_INDICATOR"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="PROCESS_DATE"><expression>[SQL7].[PROCESS_DATE]</expression></dataItem><dataItem name="HOUR"><expression>[SQL7].[HOUR]</expression></dataItem><dataItem name="Pickers"><expression>[SQL7].[USER_ID]</expression></dataItem><dataItem name="GOAL"><expression>[SQL7].[GOAL]</expression></dataItem><dataItem name="ORDER_TYPE"><expression>[SQL7].[ORDER_TYPE]</expression></dataItem><dataItem name="ORDER_LINE_INV_TYPE"><expression>[SQL7].[ORDER_LINE_INV_TYPE]</expression></dataItem><dataItem name="LOCATION"><expression>[SQL7].[LOCATION]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL7].[FACILITY]</expression></dataItem><dataItem name="FOOTWEAR_INDICATOR"><expression>[SQL7].[FOOTWEAR_INDICATOR]</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="HOUR" aggregateMethod="total" name="Total(HOUR)" label="Total"/><dataItem name="Units Picked" aggregate="total"><expression>Total([SQL7].[PICKED_QTY])</expression></dataItem></selection>
		<detailFilters><detailFilter><filterExpression>[FACILITY] in ?pFacility?</filterExpression></detailFilter><detailFilter><filterExpression>[FOOTWEAR_INDICATOR] in ?pFootwearIndicator?</filterExpression></detailFilter></detailFilters></query></queries><namedConditionalStyles><advancedConditionalStyle name="Conditional Formatting - DO Allocated"><styleCases><styleCase><style><defaultStyles><defaultStyle refStyle="pd_4"/></defaultStyles><CSS value="background-color:yellow;color:black"/></style><reportCondition>[CombinedQry].[DO Allocated] &gt; 0</reportCondition></styleCase></styleCases><styleDefault/></advancedConditionalStyle><advancedConditionalStyle name="Conditional Formatting - Wave Status"><styleCases><styleCase><style><CSS value="background-color:yellow"/></style><reportCondition>[CombinedQry].[Wave Status] = 'Wave Open'</reportCondition></styleCase></styleCases><styleDefault/></advancedConditionalStyle><advancedConditionalStyle name="% Utilized - Formatting"><styleCases><styleCase><style><defaultStyles><defaultStyle refStyle="pd_5"/></defaultStyles></style><reportCondition>[ActiveCapacity].[% Utilized] &gt;0.95</reportCondition></styleCase><styleCase><style><CSS value="background-color:yellow;color:black"/></style><reportCondition>[ActiveCapacity].[% Utilized]&gt;.85 and [ActiveCapacity].[% Utilized]&lt;=.95</reportCondition></styleCase></styleCases><styleDefault><style><defaultStyles><defaultStyle refStyle="pd_1"/></defaultStyles></style></styleDefault></advancedConditionalStyle><stringsConditionalStyle name="Conditional Style 1"><conditionalDataItem refQuery="thruput_smry" refDataItem="PACKED_ON_TRACK"/><stringConditions><stringValueList selectedValues="false"><style><defaultStyles><defaultStyle refStyle="pd_1"/></defaultStyles></style><styleValues><styleValue>On track</styleValue></styleValues></stringValueList></stringConditions><styleDefault><style><defaultStyles><defaultStyle refStyle="pd_5"/></defaultStyles><CSS value="background-color:rgb(255,122,122)"/></style></styleDefault></stringsConditionalStyle><stringsConditionalStyle name="Conditional Style 2"><conditionalDataItem refQuery="thruput_smry" refDataItem="PICKED_ON_TRACK"/><stringConditions><stringValueList selectedValues="false"><style><defaultStyles><defaultStyle refStyle="pd_1"/></defaultStyles></style><styleValues><styleValue>On track</styleValue></styleValues></stringValueList></stringConditions><styleDefault><style><defaultStyles><defaultStyle refStyle="pd_5"/></defaultStyles><CSS value="background-color:rgb(255,122,122)"/></style></styleDefault></stringsConditionalStyle></namedConditionalStyles><classStyles><classStyle name="tt"><CSS value="font-family:Arial;font-size:20pt;font-weight:bold;color:#C00720"/></classStyle><classStyle name="cls1" label="SectionHeader"><CSS value="padding:4px 5px 4px 5px;background-color:#333333;color:white;font-size:10pt;font-weight:bold;vertical-align:top"/></classStyle></classStyles></report>
