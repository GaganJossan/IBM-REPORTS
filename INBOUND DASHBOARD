		</query><query name="Recv">
			<source>
				
			<sqlQuery name="SQL3" dataSource="WMOS">
			<sqlText>SELECT DISTINCT
    ptt.user_id AS "User ID",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR) AS "date and time",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'DD-MON-YYYY') AS "Date",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'HH24:MI:SS') AS "Time",
    ptt.cntr_nbr AS "LPN",
    lh.dsp_locn AS "To Locn",
    CASE 
        WHEN lh.dsp_locn in ('STA-GE-DK-001') THEN 'Milton'
        WHEN lh.dsp_locn in ('STA-GE-00-001') THEN 'Mississauga'
        ELSE lh.work_grp
    END AS Facility
FROM PROD_TRKG_TRAN ptt
LEFT JOIN locn_hdr lh ON ptt.TO_LOCN = lh.locn_id
LEFT JOIN locn_hdr lhh ON ptt.FROM_Locn = lhh.locn_id
WHERE ptt.MENU_OPTN_NAME IN ('Recv iLPN ASN', 'Recv iLPN ASN mixed', 'Recv Item Lvl')
    AND TRAN_TYPE = '200'
    AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &gt;= CAST(TO_DATE(SUBSTR(#Prompt('pStartDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
    AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &lt;= CAST(TO_DATE(SUBSTR(#Prompt('pEndDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
    AND CASE 
            WHEN lh.dsp_locn IN ('STA-GE-DK-001') THEN 'Milton'
            WHEN lh.dsp_locn IN ('STA-GE-00-001') THEN 'Mississauga'
            ELSE lh.work_grp
        END IN (#Promptmany('pTote', 'string', '''Milton'', ''Mississauga''')#)</sqlText>
		<mdProjectedItems><mdProjectedItem name="User ID"/><mdProjectedItem name="date and time"/><mdProjectedItem name="Date"/><mdProjectedItem name="Time"/><mdProjectedItem name="LPN"/><mdProjectedItem name="To Locn"/><mdProjectedItem name="FACILITY"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="User ID"><expression>[SQL3].[User ID]</expression></dataItem><dataItem name="date and time"><expression>[SQL3].[date and time]</expression></dataItem><dataItem name="Date"><expression>[SQL3].[Date]</expression></dataItem><dataItem name="Time"><expression>[SQL3].[Time]</expression></dataItem><dataItem name="LPN"><expression>[SQL3].[LPN]</expression></dataItem><dataItem name="To Locn"><expression>[SQL3].[To Locn]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL3].[FACILITY]</expression></dataItem></selection>
		</query><query name="Facility">
			<source>
				
			<sqlQuery name="SQL4" dataSource="WMOS">
			<sqlText>select case when rownum = 1 then 'Milton'
	when rownum = 2 then 'Mississauga'
	end FACILITY
from dual connect by rownum &lt;= 2</sqlText>
		<mdProjectedItems><mdProjectedItem name="FACILITY"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="FACILITY"><expression>[SQL4].[FACILITY]</expression></dataItem></selection>
		</query><query name="SORT">
			<source>
				
			<sqlQuery name="SQL5" dataSource="WMOS">
			<sqlText>SELECT DISTINCT
    ptt.user_id AS "User ID",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR) AS "date and time",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'DD-MON-YYYY') AS "Date",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'HH24:MI:SS') AS "Time",
    ptt.cntr_nbr AS "LPN",
    lh.dsp_locn AS "To Locn",
    CASE 
        WHEN lh.zone = 'Z1' THEN 'Mississauga'
        WHEN lh.zone = 'Z2' THEN 'Milton'
        ELSE 'Other'
    END AS Facility
FROM PROD_TRKG_TRAN ptt
LEFT JOIN locn_hdr lh ON ptt.TO_LOCN = lh.locn_id
LEFT JOIN locn_hdr lhh ON ptt.FROM_Locn = lhh.locn_id
WHERE ptt.MENU_OPTN_NAME IN ('Sort iLPN', 'Sort iLPN Rtn', 'Sort iLPN RtnC', 'Sort iLPN RtnE', 'Sort iLPN mix')
  AND TRAN_TYPE = '200'
  AND (lh.dsp_locn LIKE 'PZ2%' OR lh.dsp_locn LIKE 'PZ1%')
  AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &gt;= CAST(TO_DATE(SUBSTR(#Prompt('pStartDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
  AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &lt;= CAST(TO_DATE(SUBSTR(#Prompt('pEndDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
  AND CASE 
          WHEN lh.zone = 'Z1' THEN 'Mississauga'
          WHEN lh.zone = 'Z2' THEN 'Milton'
          ELSE 'Other'
    END IN (#Promptmany('pTote', 'string', '''Milton'', ''Mississauga''')#)
</sqlText>
		<mdProjectedItems><mdProjectedItem name="User ID"/><mdProjectedItem name="date and time"/><mdProjectedItem name="Date"/><mdProjectedItem name="Time"/><mdProjectedItem name="LPN"/><mdProjectedItem name="To Locn"/><mdProjectedItem name="FACILITY"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="User ID"><expression>[SQL5].[User ID]</expression></dataItem><dataItem name="date and time"><expression>[SQL5].[date and time]</expression></dataItem><dataItem name="Date"><expression>[SQL5].[Date]</expression></dataItem><dataItem name="Time"><expression>[SQL5].[Time]</expression></dataItem><dataItem name="LPN"><expression>[SQL5].[LPN]</expression></dataItem><dataItem name="To Locn"><expression>[SQL5].[To Locn]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL5].[FACILITY]</expression></dataItem></selection>
		</query><query name="pUTAWAY">
			<source>
				
			<sqlQuery name="SQL6" dataSource="WMOS">
			<sqlText>SELECT DISTINCT
    ptt.user_id AS "User ID",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR) AS "date and time",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'DD-MON-YYYY') AS "Date",
    TO_CHAR(ptt.END_DATE + INTERVAL '3' HOUR, 'HH24:MI:SS') AS "Time",
    ptt.cntr_nbr AS "LPN",
    lhh.dsp_locn AS "From Locn", 
    lh.dsp_locn AS "To Locn",
    CASE 
        WHEN lhh.dsp_locn = 'PUT-LA-NE-002' THEN 'Mississauga'
        WHEN lhh.dsp_locn = 'PUT-LA-NE-005' THEN 'Milton'
        ELSE 'Other'
    END AS Facility
FROM PROD_TRKG_TRAN ptt
INNER JOIN locn_hdr lh ON ptt.TO_LOCN = lh.locn_id
INNER JOIN locn_hdr lhh ON ptt.FROM_Locn = lhh.locn_id
WHERE ptt.MENU_OPTN_NAME IN ('Locate iLPN')
  AND TRAN_TYPE = '200'
  AND (lhh.dsp_locn IN ('PUT-LA-NE-002') OR lhh.dsp_locn IN ('PUT-LA-NE-005'))
  AND lh.work_grp IN ('MRAK','CRAK','PRAK','MPDK')
  AND lh.dsp_locn NOT LIKE 'PUT%'
  AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &gt;= CAST(TO_DATE(SUBSTR(#Prompt('pStartDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
  AND CAST(ptt.END_DATE + INTERVAL '3' HOUR AS timestamp) &lt;= CAST(TO_DATE(SUBSTR(#Prompt('pEndDateTime')#, 0, 19), 'YYYY-MM-DD"T"HH24:mi:ss') AS timestamp)
  AND CASE 
          WHEN lhh.dsp_locn = 'PUT-LA-NE-002' THEN 'Mississauga'
          WHEN lhh.dsp_locn = 'PUT-LA-NE-005' THEN 'Milton'
          ELSE 'Other'
    END IN (#Promptmany('pTote', 'string', '''Milton'', ''Mississauga''')#)
</sqlText>
		<mdProjectedItems><mdProjectedItem name="User ID"/><mdProjectedItem name="date and time"/><mdProjectedItem name="Date"/><mdProjectedItem name="Time"/><mdProjectedItem name="LPN"/><mdProjectedItem name="From Locn"/><mdProjectedItem name="To Locn"/><mdProjectedItem name="FACILITY"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="User ID"><expression>[SQL6].[User ID]</expression></dataItem><dataItem name="date and time"><expression>[SQL6].[date and time]</expression></dataItem><dataItem name="Date"><expression>[SQL6].[Date]</expression></dataItem><dataItem name="Time"><expression>[SQL6].[Time]</expression></dataItem><dataItem name="LPN"><expression>[SQL6].[LPN]</expression></dataItem><dataItem name="From Locn"><expression>[SQL6].[From Locn]</expression></dataItem><dataItem name="To Locn"><expression>[SQL6].[To Locn]</expression></dataItem><dataItem name="FACILITY"><expression>[SQL6].[FACILITY]</expression></dataItem></selection>
		</query></queries></report>
